{
  "name": "aide",
  "displayName": "AIDE - Intent Pipeline",
  "publisher": "platysonique",
  "version": "1.5.4",
  "description": "Intent \u2192 Tool \u2192 Execution pipeline for natural language development automation",
  "repository": {
    "type": "git",
    "url": "https://github.com/platysonique/AIDE.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aideChatContainer",
          "title": "AIDE",
          "icon": "$(robot)"
        }
      ]
    },
    "views": {
      "aideChatContainer": [
        {
          "type": "webview",
          "id": "aide.chatView",
          "name": "Chat",
          "when": "true"
        },
        {
          "type": "webview",
          "id": "aide.toolsView",
          "name": "Tools",
          "when": "true"
        }
      ]
    },
    "commands": [
      {
        "command": "aide.intentExecute",
        "title": "AIDE: Execute Intent \u2192 Tool \u2192 Action",
        "icon": "$(play)"
      },
      {
        "command": "aide.formatCode",
        "title": "AIDE: Format My Code",
        "icon": "$(symbol-operator)"
      },
      {
        "command": "aide.fixErrors",
        "title": "AIDE: Fix My Errors",
        "icon": "$(debug-alt)"
      },
      {
        "command": "aide.runTests",
        "title": "AIDE: Run My Tests",
        "icon": "$(beaker)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run bundle",
    "compile": "tsc -p .",
    "bundle": "webpack --mode production",
    "package": "vsce package --out aide.vsix"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.x",
    "ts-loader": "^9.4.0",
    "typescript": "^5.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "@vscode/vsce": "^2.23.0"
  },
  "homepage": "https://github.com/platysonique/AIDE"
}
